<?xml version="1.0" encoding="utf-8"?>
<manifest package="cn.dlc.fbrtest"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="vendingcabinets.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="design_width_in_dp"
            android:value="720"/>
        <meta-data
            android:name="design_height_in_dp"
            android:value="1280"/>
        <activity android:name="vendingcabinets.dlc.cn.vendingcabinets.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="vendingcabinets.dlc.cn.vendingcabinets.TestActivity"></activity>
        <activity android:name="vendingcabinets.dlc.cn.vendingcabinets.TaskQueueActivity" />
        <activity android:name="vendingcabinets.dlc.cn.vendingcabinets.TestSendActivity" />
        <activity android:name="serialtool.activity.MainActivity" />
        <activity android:name="serialtool.activity.LoadCmdListActivity" />
    </application>

</manifest>